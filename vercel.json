{
    "version": 2,
    "installCommand": "chmod +x build.sh && ./build.sh setup",
    "buildCommand": "./build.sh build",
    "outputDirectory": "dist",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/((?!assets/|canvaskit/|icons/|favicon.png|manifest.json|main.dart.js|flutter.js|flutter_bootstrap.js|flutter_service_worker.js|version.json|build_info.txt).*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/"
                }
            ]
        }
    ]
}