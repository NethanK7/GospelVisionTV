{
    "version": 2,
    "installCommand": "chmod +x build.sh && ./build.sh setup",
    "buildCommand": "./build.sh build",
    "outputDirectory": "public",
    "rewrites": [
        {
            "source": "/((?!api/|assets/|canvaskit/|icons/|favicon.png|manifest.json|main.dart.js|flutter.js|flutter_bootstrap.js|flutter_service_worker.js|version.json|build_info.txt).*)$",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "credentialless"
                },
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                },
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/"
                }
            ]
        },
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}